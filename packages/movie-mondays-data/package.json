{
  "name": "movie-mondays-data",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node --inspect=5858 -r ts-node/register . localhost 8080",
    "start:watch": "nodemon",
    "test": "jest --watchAll"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "ext": "ts",
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "better-sqlite3": "^7.1.4",
    "jest": "^26.6.3",
    "jsdom": "15.1.1",
    "movie-mondays-db-queries": "file:../movie-mondays-db-queries",
    "node-fetch": "^2.6.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.4"
  }
}
